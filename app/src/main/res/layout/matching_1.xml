<HorizontalScrollView xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="wrap_content"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	tools:ignore="MissingClass">

	<ScrollView android:id="@+id/matching_1" 
		android:layout_width="wrap_content" 
		android:layout_height="match_parent" >

		<RelativeLayout android:id="@+id/content_container" 
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content" >


			<RelativeLayout android:id="@+id/page_matching_1_ek1"
				android:layout_marginTop="0dp"
				android:layout_marginStart="0dp"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				>

				<com.airbnb.lottie.LottieAnimationView
					android:id="@+id/lottie"
					android:layout_width="400dp"
					android:layout_height="400dp"
					android:layout_alignParentStart="true"
					android:layout_marginStart="-21dp"
					android:layout_marginTop="120dp"
					app:lottie_autoPlay="true"
					app:lottie_fileName="lf30_editor_popzctkg.json"
					app:lottie_loop="true" />

				<View
					android:id="@+id/_bg__matching_1_ek2"
					android:layout_width="360dp"
					android:layout_height="640dp"
					android:layout_marginStart="0dp"
					android:layout_marginTop="0dp"
					android:background="@drawable/_bg__matching_1_ek2_shape" />

				<ImageView
					android:id="@+id/matching"
					android:layout_width="156dp"
					android:layout_height="156dp"
					android:layout_marginStart="102dp"
					android:layout_marginTop="242dp"
					android:contentDescription="matching"
					android:src="@drawable/___2_1" />

				<TextView
					android:id="@+id/wait"
					android:layout_width="152dp"
					android:layout_height="41dp"
					android:layout_gravity="center_horizontal|center"
					android:layout_marginLeft="109dp"
					android:layout_marginTop="140dp"
					android:gravity="center"
					android:shadowColor="#000000"
					android:shadowDx="0"
					android:shadowDy="4"
					android:shadowRadius="4"
					android:text=""
					android:textColor="@color/_bg__matching_1_ek2_color"
					android:textSize="30sp" />


				<RelativeLayout android:id="@+id/component"
					android:layout_marginTop="560dp"
					android:layout_marginStart="12dp"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					>


					<RelativeLayout android:id="@+id/menu"
						android:layout_marginTop="0dp"
						android:layout_marginStart="0dp"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						>

						<View   android:id="@+id/background"
							android:background="@drawable/background_shape"
							 android:layout_marginStart="0dp"
							android:layout_marginTop="0dp"
							android:layout_width="335dp"
							android:layout_height="62dp" />


						<RelativeLayout android:id="@+id/home"
							android:layout_marginTop="9dp"
							android:layout_marginStart="24dp"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							>


							<RelativeLayout android:id="@+id/heart"
								android:layout_marginTop="0dp"
								android:layout_marginStart="0dp"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								>

								<View
									android:id="@+id/rectangle_34"
									android:layout_width="42dp"
									android:layout_height="42dp"
									android:layout_marginStart="0dp"
									android:layout_marginTop="0dp"
									android:background="@drawable/rectangle_34_shape" />


								<RelativeLayout android:id="@+id/_1__icons___fillter____bookmark"
									android:layout_marginTop="6dp"
									android:layout_marginStart="4dp"
									android:layout_width="wrap_content"
									android:layout_height="wrap_content"
									>

									<ImageView
										android:id="@+id/heart_ek1"
										android:layout_width="26.25dp"
										android:layout_height="22dp"
										android:layout_marginStart="4.38dp"
										android:layout_marginTop="5.5dp"
										android:contentDescription="heart_ek1"
										android:src="@drawable/heart_ek1" />

								</RelativeLayout>

							</RelativeLayout>

							<View
								android:id="@+id/rectangle_34_ek1"
								android:layout_width="42dp"
								android:layout_height="42dp"
								android:layout_marginStart="122dp"
								android:layout_marginTop="1dp"
								android:background="@drawable/rectangle_34_ek1_shape" />


							<RelativeLayout android:id="@+id/_1__icons___fillter____bookmark_copy"
								android:layout_marginTop="7dp"
								android:layout_marginStart="125dp"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								>

								<ImageView android:id="@+id/home_ek1"
									android:src="@drawable/home_ek1"
									android:contentDescription="home_ek1"
									 android:layout_marginStart="4.24dp"
									android:layout_marginTop="3.75dp"
									android:layout_width="25.43dp"
									android:layout_height="22.5dp" />

							</RelativeLayout>

						</RelativeLayout>


						<RelativeLayout android:id="@+id/profile"
							android:layout_marginTop="9dp"
							android:layout_marginStart="270dp"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							>

							<View
								android:id="@+id/rectangle_30"
								android:layout_width="42dp"
								android:layout_height="42dp"
								android:layout_marginStart="0dp"
								android:layout_marginTop="0dp"
								android:background="@drawable/rectangle_30_shape" />

							<ImageView
								android:id="@+id/icon"
								android:layout_width="31dp"
								android:layout_height="31dp"
								android:layout_marginStart="5dp"
								android:layout_marginTop="7dp"
								android:contentDescription="icon"
								android:src="@drawable/profileicon" />

						</RelativeLayout>

					</RelativeLayout>

				</RelativeLayout>

				<View   android:id="@+id/ellipse_9"
					android:background="@drawable/ellipse_9_shape"
					 android:alpha="0.60000002384186"
					 android:layout_marginStart="69dp"
					android:layout_marginTop="567dp"
					android:layout_width="13dp"
					android:layout_height="13dp" />

				<TextView
					android:id="@+id/_5"
					android:layout_width="20dp"
					android:layout_height="16.5dp"
					android:layout_gravity="center_horizontal|center"
					android:layout_marginLeft="65dp"
					android:layout_marginTop="564dp"
					android:gravity="center"
					android:text="@string/_5_string"
					android:textColor="@color/background_color" />

			</RelativeLayout>



		</RelativeLayout>

	</ScrollView>
</HorizontalScrollView>

